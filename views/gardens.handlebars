<header>
   {{#if loggedIn}}
   <h1>you are logged in.</h1>
   <p>your username is: {{username}}</p>
   {{/if}}
</header>
<main>
   <h1>Gardens</h1>
   <br>
   <br>
   <hr>


   <h1>Add Garden</h1>

   <div class="container">
      <form id="add-garden-form">
  <div class="form-group row" bis_skin_checked="1">
    <label for="garden-name" class="col-sm-2 col-form-label">Garden Name</label>
    <div class="col-sm-10" bis_skin_checked="1">
      <input type="text" class="form-control" id="garden-name" placeholder="Tomato Bed">
    </div>
  </div>
  <div class="form-group row" bis_skin_checked="1">
    <label for="garden-location" class="col-sm-2 col-form-label">Garden Location (ZIP)</label>
    <div class="col-sm-10" bis_skin_checked="1">
      <input type="text" class="form-control" id="garden-location" placeholder="07013">
    </div>
  <div class="form-group row" bis_skin_checked="1">
    <label for="planted-date" class="col-sm-2 col-form-label">Planted Date</label>
    <div class="col-sm-10" bis_skin_checked="1">
      <input type="text" class="form-control" id="planted-date" placeholder="05-01-2019">
    </div>
  </div>
  <div class="form-group row" bis_skin_checked="1">
    <div class="col-sm-10 offset-sm-2" bis_skin_checked="1">
      <button type="submit" class="btn btn-primary">Add</button>
    </div>
  </div>
</form>


   </div>
</main>

<script src="../assets/js/app.js"></script>